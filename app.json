{
  "expo": {
    "name": "Cache Mobo App",
    "slug": "cache_mobo_app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#212121"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.CacheCore.CacheMoboApp"
    },
    "android": {
      "package": "com.CacheCore.CacheMoboApp",
      "versionCode": 27,
      "permissions": [
        "CAMERA",
        "USE_FINGERPRINT"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#212529",
      "translucent": false
    },
    "description": "Secure mobile wallet for the $CXCHE blockchain."
  }
}